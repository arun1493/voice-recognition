{
  "version": 2,
  "projects": {
    "admin-admin": {
      "root": "apps/admin/admin",
      "sourceRoot": "apps/admin/admin/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/admin/admin",
            "index": "apps/admin/admin/src/index.html",
            "baseHref": "/",
            "main": "apps/admin/admin/src/main.tsx",
            "polyfills": "apps/admin/admin/src/polyfills.ts",
            "tsConfig": "apps/admin/admin/tsconfig.app.json",
            "assets": ["apps/admin/admin/src/favicon.ico", "apps/admin/admin/src/assets"],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/admin/admin/src/environments/environment.ts",
                  "with": "apps/admin/admin/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "admin-admin:build",
            "hmr": true
          },
          "configurations": {
            "production": {
              "buildTarget": "admin-admin:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/admin/admin/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/admin/admin"],
          "options": {
            "jestConfig": "apps/admin/admin/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "admin-admin-e2e": {
      "root": "apps/admin/admin-e2e",
      "sourceRoot": "apps/admin/admin-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/admin/admin-e2e/cypress.json",
            "devServerTarget": "admin-admin:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "admin-admin:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/admin/admin-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "physician-physician": {
      "root": "apps/physician/physician",
      "sourceRoot": "apps/physician/physician/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/physician/physician",
            "index": "apps/physician/physician/src/index.html",
            "baseHref": "/",
            "main": "apps/physician/physician/src/main.tsx",
            "polyfills": "apps/physician/physician/src/polyfills.ts",
            "tsConfig": "apps/physician/physician/tsconfig.app.json",
            "assets": ["apps/physician/physician/src/favicon.ico", "apps/physician/physician/src/assets"],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/physician/physician/src/environments/environment.ts",
                  "with": "apps/physician/physician/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "4mb",
                  "maximumError": "6mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "physician-physician:build",
            "hmr": true,
            "port": 3003,
            "scripts": []
          },
          "configurations": {
            "production": {
              "buildTarget": "physician-physician:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/physician/physician/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/physician/physician"],
          "options": {
            "jestConfig": "apps/physician/physician/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "physician-physician-e2e": {
      "root": "apps/physician/physician-e2e",
      "sourceRoot": "apps/physician/physician-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/physician/physician-e2e/cypress.json",
            "devServerTarget": "physician-physician:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "physician-physician:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/physician/physician-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "suki-business": {
      "root": "libs/suki-business",
      "sourceRoot": "libs/suki-business/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/suki-business/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/suki-business"],
          "options": {
            "jestConfig": "libs/suki-business/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "suki-editor": {
      "root": "libs/suki-editor",
      "sourceRoot": "libs/suki-editor/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/suki-editor/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/suki-editor"],
          "options": {
            "jestConfig": "libs/suki-editor/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "suki-ui": {
      "root": "libs/suki-ui",
      "sourceRoot": "libs/suki-ui/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["libs/suki-ui/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/suki-ui"],
          "options": {
            "jestConfig": "libs/suki-ui/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/suki-ui/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/suki-ui",
            "config": {
              "configFolder": "libs/suki-ui/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "suki-ui-e2e": {
      "root": "apps/suki-ui-e2e",
      "sourceRoot": "apps/suki-ui-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/suki-ui-e2e/cypress.json",
            "devServerTarget": "suki-ui:storybook"
          },
          "configurations": {
            "ci": {
              "devServerTarget": "suki-ui:storybook:ci"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/suki-ui-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "web": {
      "root": "apps/web",
      "sourceRoot": "apps/web/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/web",
            "main": "apps/web/src/main.ts",
            "tsConfig": "apps/web/tsconfig.app.json",
            "assets": ["apps/web/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/web/src/environments/environment.ts",
                  "with": "apps/web/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "web:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/web/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/web"],
          "options": {
            "jestConfig": "apps/web/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "styled-components"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "physician-physician"
}
